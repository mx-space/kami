{
  "name": "mx-web",
  "license": "GPL-3.0",
  "private": false,
  "version": "3.0.0-alpha.1",
  "engines": {
    "node": ">=14"
  },
  "bump": {
    "before": [
      "git pull --rebase"
    ],
    "after": [
      "sh ./scripts/create-tags.sh"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.gitignore --write ",
      "eslint --cache"
    ]
  },
  "scripts": {
    "start": "npm run dev",
    "prebuild": "rimraf .next",
    "dev": "cross-env NODE_ENV=development next dev -p 2323",
    "analyze": "cross-env NODE_ENV=production ANALYZE=true BUNDLE_ANALYZE=browser next build",
    "build": "cross-env NODE_ENV=production next build",
    "build:ci": "cross-env NODE_ENV=production CI=true next build",
    "lint": "eslint --ext .ts,.tsx --ignore-path .gitignore . --fix",
    "stylelint": "stylelint 'src/**/*.{css,scss}' --fix",
    "prod:pm2": "cross-env NODE_ENV=production pm2 restart ecosystem.config.js",
    "prod:stop": "pm2 stop ecosystem.config.js",
    "prod:reload": "pm2 reload ecosystem.config.js",
    "gen-config": "node scripts/generate-default-configs.js"
  },
  "browserslist": [
    "chrome >=88",
    "safari >=14",
    "last 2 Firefox versions"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@innei/devtools-detector": "2.0.4-beta.1",
    "@innei/react-stack-grid": "0.8.4",
    "@mx-space/api-client": "0.5.3",
    "@mx-space/extra": "0.4.0",
    "NeteaseCloudMusicApi": "*",
    "axios": "0.24.0",
    "camelcase-keys": "7.0.1",
    "clsx": "1.1.1",
    "dayjs": "1.10.7",
    "inflection": "1.13.1",
    "js-cookie": "3.0.1",
    "lodash-es": "4.17.21",
    "mdurl": "*",
    "mobx": "6.3.12",
